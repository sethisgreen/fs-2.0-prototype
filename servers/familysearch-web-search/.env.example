# FamilySearch Web Search MCP Server Configuration

# Custom user agent to bypass Imperva firewall
# Replace with your custom user agent that can bypass the firewall
FAMILYSEARCH_WEB_CUSTOM_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"

# Rate limiting settings
FAMILYSEARCH_WEB_REQUESTS_PER_MINUTE=30
FAMILYSEARCH_WEB_REQUESTS_PER_HOUR=1000
FAMILYSEARCH_WEB_DELAY_BETWEEN_REQUESTS=2.0

# Server settings
FAMILYSEARCH_WEB_SERVER_NAME="familysearch-web-search"
FAMILYSEARCH_WEB_SERVER_URL="http://localhost:8002/mcp"
FAMILYSEARCH_WEB_SERVER_PORT=8002

# Caching settings
FAMILYSEARCH_WEB_CACHE_TTL=3600
FAMILYSEARCH_WEB_MAX_CACHE_SIZE=1000

# Search settings
FAMILYSEARCH_WEB_DEFAULT_MAX_RESULTS=20
FAMILYSEARCH_WEB_MAX_RESULTS_LIMIT=100

# Timeout settings
FAMILYSEARCH_WEB_REQUEST_TIMEOUT=30.0
FAMILYSEARCH_WEB_CONNECTION_TIMEOUT=10.0

# Retry settings
FAMILYSEARCH_WEB_MAX_RETRIES=3
FAMILYSEARCH_WEB_RETRY_DELAY=1.0
FAMILYSEARCH_WEB_RETRY_BACKOFF=2.0

# Logging settings
FAMILYSEARCH_WEB_LOG_LEVEL="INFO"
FAMILYSEARCH_WEB_LOG_FORMAT="json"